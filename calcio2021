###  http://www.semanticweb.org/morellinuzzarello/progetto/calcio2021#AllenatoreFamoso
:AllenatoreFamoso rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                               owl:onProperty :allenaSquadra ;
                                                               owl:someValuesFrom :Club
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :haAllenato ;
                                                               owl:someValuesFrom :Nazionale
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Allenatore ;
                  rdfs:comment "Allenatore che allena Club e Nazionali" ;
                  rdfs:label "AllenatoreFamoso" .


