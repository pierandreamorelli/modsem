###  http://www.semanticweb.org/morellinuzzarello/progetto/calcio2021#GiovanePromessa
:GiovanePromessa rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                              owl:onProperty :giocaInSquadra ;
                                                              owl:someValuesFrom :TopClub
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :anni ;
                                                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                   owl:onDatatype xsd:integer ;
                                                                                   owl:withRestrictions ( [ xsd:maxExclusive 22
                                                                                                          ]
                                                                                                        )
                                                                                 ]
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :Calciatore ;
                 rdfs:comment "Calciatore che gioca in TopClub e ha meno di 22 anni" ;
                 rdfs:label "GiovanePromessa" .


