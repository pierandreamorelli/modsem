###  http://www.semanticweb.org/morellinuzzarello/progetto/calcio2021#TopClub
:TopClub rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :vinceTrofeo ;
                               owl:minQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                               owl:onClass :TrofeoClub
                             ] ;
         rdfs:subClassOf :Club ;
         rdfs:comment "Club che ha vinto minimo 5 TrofeoClub" ;
         rdfs:label "TopClub" .


###  http://www.semanticweb.org/morellinuzzarello/progetto/calcio2021#TopClubEU
:TopClubEU rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :vinceTrofeo ;
                                 owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                 owl:onClass :TrofeoClubEU
                               ] ;
           rdfs:subClassOf :TopClub ;
           rdfs:comment "Club che vince minimo 3 TrofeoClubEU" ;
           rdfs:label "TopClubEU" .


